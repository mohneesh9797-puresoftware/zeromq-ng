    "benchmark": ">= 0",
    "chai": ">= 4.1",
    "choma": ">= 1.2",
    "eslint": ">= 4.0",
    "mocha": ">= 4.0",
    "prebuildify": "lgeiger/prebuildify#libc",
    "prebuildify-ci": "^1.0.2",
    "semver": ">= 0",
    "ts-node": ">= 7",
    "typescript": ">= 3",
    "weak": ">= 1.0"
  },
  "engines": {
    "node": ">= 8.8"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
