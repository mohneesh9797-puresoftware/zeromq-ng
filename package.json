{
  "name": "zeromq-ng",
  "version": "5.0.0-beta.21",
  "description": "Next-generation ZeroMQ bindings for Node.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "gypfile": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/rolftimmermans/zeromq-ng.git"
  },
  "dependencies": {
    "node-addon-api": "^1.7.1",
    "node-gyp-build": "^4.1.0"
  },
  "devDependencies": {
    "@types/chai": ">= 4.1",
    "@types/mocha": ">= 5.2",
    "@types/node": ">= 8.0",
    "@types/semver": ">= 0",
    "benchmark": ">= 0",
    "chai": ">= 4.1",
    "choma": ">= 1.2",
    "gunzip-maybe": "^1.4.1",
    "mocha": ">= 4.0",
    "node-fetch": "^2.6.0",
    "prebuildify": "^3.0",
    "semver": ">= 0",
    "tar-fs": "^2.0.0",
    "ts-node": ">= 7",
    "tslint": "^5.20.0",
    "typescript": ">= 3",
    "weak-napi": ">= 1.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "binding.gyp",
    "compat.js",
    "compat.d.ts",
    "lib/*.d.ts",
    "lib/*.js",
    "src/util/*.h",
    "src/*.cc",
    "src/*.h",
    "script/build.sh",
    "prebuilds"
  ],
  "scripts": {
    "install": "node-gyp-build",
    "ci:prebuild": "prebuildify --napi --strip",
    "dev:test": "script/format.sh && tslint -t stylish --project . && tsc --project . && mocha && rm -f tmp/*",
    "dev:bench": "node --harmony --expose-gc --require ./test/ts test/bench"
  },
  "keywords": [
    "zeromq",
    "zmq",
    "0mq",
