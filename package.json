  "types": "lib/index.d.ts",
  "gypfile": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/rolftimmermans/zeromq-ng.git"
  },
  "dependencies": {
    "node-addon-api": "rolftimmermans/node-addon-api#deferred-env",
    "node-gyp-build": "lgeiger/node-gyp-build#libc"
  },
  "devDependencies": {
    "@types/chai": ">= 4.1",
