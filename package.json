    "libzmq",
    "zmtp",
    "message",
    "messaging",
    "queue",
    "async",
    "sockets",
    "native",
    "binding",
    "addon",
    "napi"
  ],
  "license": "MIT",
  "author": "Rolf Timmermans <rolf@zxcv.nl>"
}
